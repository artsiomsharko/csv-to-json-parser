{
  "name": "csv-to-json-parser",
  "version": "1.0.0",
  "description": "CSV to JSON parser with uploading result on Google Drive",
  "main": "src/index.js",
  "scripts": {
    "parse": "node ./src/index.js",
    "small": "node ./src/index.js --sourceFile \"assets/small.csv\" --resultFile \"assets/small.json\" --separator \",\"",
    "middle": "node ./src/index.js --sourceFile \"assets/middle.csv\" --resultFile \"assets/middle.json\"",
    "d1": "node ./src/index.js --sourceFile \"D://assets/test.csv\" --resultFile \"D://assets/test.json\"",
    "d2": "node ./src/index.js --sourceFile \"D://assets/large.csv\" --resultFile \"D://assets/large.json\"",
    "generate": "node ./test/mockData.js"
  },
  "author": "Artem Sharko <artsiom.sharko@innowise-group.com>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.1.3",
    "googleapis": "^118.0.0",
    "progress": "^2.0.3"
  }
}
